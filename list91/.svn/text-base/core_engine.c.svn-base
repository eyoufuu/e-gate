static inline u32 core_iterate_ip(u32 ip,   u32 id)
{
	struct hlist_node * pos=NULL;
	struct core_ip * n=NULL;
	hlist_for_each(pos, &g_core_pro_ip[id]) 
	{
		n = (struct core_ip*)hlist_entry(pos, struct core_ip, hashnode);
		
		if (n->outerip == ip) 
		{
			return n->proid ;
		}
	}

	UNTOUCHED_NUMBER;
}





